<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/placeholderFragment2"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/addFragment"
        android:name="com.example.fishhelp.AddFragment"
        android:label="AddFragment" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.fishhelp.SearchFragment"
        android:label="SearchFragment" />

    <fragment
        android:id="@+id/placeholderFragment2"
        android:name="com.example.fishhelp.ui.main.PlaceholderFragment"
        android:label="PlaceholderFragment">

        <action
            android:id="@+id/goto_addFragment"
            app:destination="@id/addFragment" />

        <action
            android:id="@+id/goto_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.fishhelp.FirstFragment"
        android:label="first_main_fragment"
        tools:layout="@layout/first_main_fragment" />

</navigation>